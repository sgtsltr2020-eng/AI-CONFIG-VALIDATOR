# ============================================
# EXISTING TOKENS (UNCHANGED)
# ============================================
GITHUB_TOKEN=ghp_xxxxx  # Still used for repository accesspective services
GEMINI_API_KEY=your_gemini_api_key_here
GROQ_API_KEY=your_groq_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Optional: ElevenLabs voice ID (default will be used if not set)
ELEVENLABS_VOICE_ID=

# OpenRouter Integration (Premium Fallback)
# Set to 'true' to enable OpenRouter as premium fallback provider
# Leave 'false' or unset to use 100% free tier (GitHub + Groq + Gemini)
ENABLE_OPENROUTER=false

# OpenRouter API Key (only needed if ENABLE_OPENROUTER=true)
# Get from: https://openrouter.ai/keys
OPENROUTER_API_KEY=y_here